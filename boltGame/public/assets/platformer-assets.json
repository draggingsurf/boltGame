  "usage": {
    "description": "How AI should use this enhanced asset registry",
    "instructions": [
      "All assets now include dimension information and targetScale values",
      "Use the targetScale for consistent sizing across all games",
      "collisionBox defines the actual gameplay collision area",
      "Character sprites target 32px height when scaled",
      "Enemy sprites target 24px average size when scaled",
      "Collectibles target 16px average size when scaled",
      "Platform tiles are designed for 32px grid alignment"
    ],
    "scalingSystem": {
      "description": "Standardized scaling system for consistent game appearance",
      "rules": [
        "Always use asset.dimensions.targetScale for sprite scaling",
        "Characters scale to ~32px gameplay height",
        "Enemies scale to ~24px average size", 
        "Collectibles scale to ~16px average size",
        "Platforms scale to align with 32px grid",
        "Use collisionBox for physics bodies, not full sprite size"
      ]
    },
    "positioningSystem": {
      "description": "Intelligent positioning system",
      "gameGrid": 32,
      "layoutRules": [
        "Position sprites on 32px grid boundaries when possible",
        "Player starts at (64, gameHeight - 96) for consistent ground placement",
        "Platforms align to 32px grid: y positions like 480, 448, 416, etc.",
        "Enemies spawn on platform surfaces using platform.y - 32",
        "Collectibles float slightly above platforms: platform.y - 48",
        "Backgrounds always center and fit to game dimensions"
      ]
    },
    "example_implementation": {
      "description": "Correct asset loading with standardized dimensions",
      "character": {
        "id": "character_yellow_idle", 
        "scale": 0.5,
        "targetSize": "32px gameplay height",
