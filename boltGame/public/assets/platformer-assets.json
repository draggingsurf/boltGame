    "backgroundSystem": {
      "description": "Use Phaser graphics for backgrounds - much better than static images",
      "method": "phaser_graphics",
      "benefits": ["crisp gradients", "no distortion", "responsive", "lightweight"],
      "examples": {
        "sky": "this.add.graphics().fillGradientStyle(0x87CEEB, 0x87CEEB, 0x98D8FF, 0x98D8FF, 1).fillRect(0, 0, 800, 600)",
        "desert": "this.add.graphics().fillGradientStyle(0xF4A460, 0xF4A460, 0xFFD700, 0xFFD700, 1).fillRect(0, 0, 800, 600)",
        "forest": "this.add.graphics().fillGradientStyle(0x228B22, 0x228B22, 0x32CD32, 0x32CD32, 1).fillRect(0, 0, 800, 600)"
